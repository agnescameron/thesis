	month = may,
	year = {1992},
	keywords = {gaming, learning, organization, simulation, System dynamics},
	pages = {137--150},
	file = {ScienceDirect Snapshot:/Users/agnes/Zotero/storage/TJX9XWZH/037722179290011W.html:text/html}
}

